{
  "name": "@fidtech-sa/chemistry-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "postbuild": "ts-node ./css-bundle.ts",
    "build-local": "npm run packagr && npm run cp-mylib-assets && cd dist && npm pack",
    "build-mylib": "npm run packagr && npm run cp-mylib-assets && cd dist && npm pack && npm publish",
    "build-mylib-styles": "cpx \"./projects/mylib/src/lib/style/**/*\" \"./dist/mylib/style\" && scss-bundle -e ./projects/mylib/src/lib/style/_style.scss -d ./dist/mylib/style/_styles.scss",
    "cp-mylib-assets": "cpx \"./src/styles.scss\" \"./dist\""
  },
  "private": false,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "highlight.js": "^9.15.10",
    "ngx-highlightjs": "^3.0.3",
    "ngx-toastr": "^11.1.2",
    "primeng": "^8.0.4",
    "prismjs": "^1.17.1"
  },
  "devDependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/cdk": "^8.2.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/material": "^8.2.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.4.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.6.0",
    "popper.js": "^1.15.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "angular-material-source": "github:angular/material",
    "bootstrap": "^4.3.1",
    "cpx": "^1.5.0",
    "rxjs": "~6.4.0",
    "scss-bundle": "^3.0.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "ngPackage": {
    "whitelistedNonPeerDependencies": [
      "bootstrap"
    ]
  }
